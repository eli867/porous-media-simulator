{
  "name": "fluid-permeability-simulator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:binary:windows": "g++ -O3 -std=c++17 -fopenmp Perm2D.cpp -o fluid_sim.exe -I.",
    "build:binary:linux": "g++ -O3 -std=c++17 -fopenmp Perm2D.cpp -o fluid_sim -I.",
    "build:binary:all": "npm run build:binary:windows && npm run build:binary:linux",
    "build:diffusivity:windows": "nvcc -std=c++17 -Xcompiler -fopenmp -Xlinker -static-libgcc -Xlinker -static-libstdc++ -o diffusivity_sim.exe main.cu",
    "build:diffusivity:linux": "nvcc -std=c++17 -Xcompiler -fopenmp -Xlinker -static-libgcc -Xlinker -static-libstdc++ -o diffusivity_sim main.cu",
    "build:diffusivity:all": "npm run build:diffusivity:windows && npm run build:diffusivity:linux",
    "build:all": "npm run build:binary:all && npm run build:diffusivity:all"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.515.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
